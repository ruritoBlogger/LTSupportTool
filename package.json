{
  "name": "LTSupportTool",
  "version": "0.0.0",
  "description": "LTのサポートツール",
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build && next export",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint -c .eslintrc.json --no-eslintrc src/**/*.{ts,tsx}",
    "lint:fix": "eslint -c .eslintrc.json --no-eslintrc src/**/*.{ts,tsx} --fix",
    "format": "prettier src/**/*.{ts,tsx}",
    "format:fix": "prettier --write src/**/*.{ts,tsx}"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint -c .eslintrc.json --no-eslintrc src/**/*.{ts,tsx} --fix",
      "prettier --write src/**/*.{ts,tsx}"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@next/bundle-analyzer": "12.2.5",
    "@types/node": "18.7.2",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "@emotion/css": "11.10.0",
    "@emotion/react": "11.10.4",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.4",
    "@hookform/resolvers": "2.9.8",
    "@mediapipe/camera_utils": "0.3.1640029074",
    "@mediapipe/drawing_utils": "0.3.1620248257",
    "@mediapipe/face_mesh": "0.4.1633559619",
    "@mui/material": "5.10.4",
    "axios": "0.27.2",
    "kalidokit": "1.1.5",
    "next": "12.2.5",
    "pixi.js": "6.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.36.0",
    "socket.io": "4.5.2",
    "socket.io-client": "4.5.2",
    "swr": "1.3.0",
    "zod": "3.19.1"
  }
}
