{
  "name": "LTSupportTool",
  "version": "0.0.0",
  "description": "LTのサポートツール",
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build && next export",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint -c .eslintrc.json --no-eslintrc src/**/*.{ts,tsx}",
    "lint:fix": "eslint -c .eslintrc.json --no-eslintrc src/**/*.{ts,tsx} --fix",
    "format": "prettier src/**/*.{ts,tsx}",
    "format:fix": "prettier --write src/**/*.{ts,tsx}"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint -c .eslintrc.json --no-eslintrc src/**/*.{ts,tsx} --fix",
      "prettier --write src/**/*.{ts,tsx}"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@next/bundle-analyzer": "12.3.4",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/three": "^0.147.0",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "eslint": "8.33.0",
    "eslint-config-next": "12.3.4",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "prettier": "2.8.3",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@emotion/css": "11.10.5",
    "@emotion/react": "11.10.5",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.5",
    "@hookform/resolvers": "2.9.11",
    "@mediapipe/camera_utils": "^0.3.1640029074",
    "@mediapipe/drawing_utils": "0.3.1620248257",
    "@mediapipe/face_mesh": "0.4.1633559619",
    "@mediapipe/holistic": "^0.5.1635989137",
    "@mui/material": "5.11.3",
    "@pixiv/three-vrm": "^0.6.7",
    "axios": "0.27.2",
    "kalidokit": "1.1.5",
    "next": "12.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.41.3",
    "socket.io": "4.5.4",
    "socket.io-client": "4.5.4",
    "three": "^0.148.0",
    "zod": "3.20.2"
  }
}
